{
	"name": "flutter-template-cli",
	"version": "0.0.0",
	"license": "MIT",
  	"private": true,
	"bin": "dist/esm/cli.js",
	"type": "module",
	"engines": {
	  "node": ">=14.16"
	},
	"scripts": {
	  "build": "tsc && chmod +x dist/esm/cli.js",
	  "start": "npm run build && node --es-module-specifier-resolution=node dist/esm/cli.js",
	  "pretest": "npm run build",
	  "test": "xo && ava"
	},
	"files": [
		"dist/esm/cli.js"
	],
	"dependencies": {
		"ink": "^3.2.0",
		"meow": "^9.0.0",
		"react": "^17.0.2",
		"cli-spinners": "^2.6.1",
		"dotenv": "^16.0.1",
	  	"react-dom": "^17.0.2"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.15",
	  	"@types/lodash": "^4.14.182",
		"ava": "^4.3.0",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.7.4",
		"xo": "^0.39.1",
	  	"@types/react-dom": "^18.0.5",
		"@commander/ui-kit": "*",
		"@commander/utilities": "*"
	},
	"nx": {
	  "targets": {
		"build": {
		  "outputs": [
			"{projectRoot}/build",
			"{projectRoot}/public/build"
		  ]
		}
	  }
	}
}
