{
  "name": "flutter-template-cli",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "bin": "dist/esm/cli.js",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/esm/cli.js",
    "start": "npm run build && node --es-module-specifier-resolution=node dist/esm/cli.js",
    "pretest": "npm run build",
    "test": "xo && ava",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\""
  },
  "files": [
    "dist/esm/cli.js"
  ],
  "dependencies": {
    "ink": "^3.2.0",
    "meow": "^9.0.0",
    "react": "^17.0.2",
    "dotenv": "^16.0.1",
    "react-dom": "^17.0.2",
    "i18next": "^21.8.14",
    "react-i18next": "^11.18.1",
	"reflect-metadata": "^0.1.13",
	"typedi": "^0.10.0",
	"fp-ts": "^2.12.1",
	"@felangel/bloc": "^0.3.0",
	"@felangel/react-bloc": "^0.3.0"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "source/": "dist/"
      }
    }
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/react": "^18.0.15",
    "@types/lodash": "^4.14.182",
    "ava": "^4.3.0",
    "chalk": "^4.1.2",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^2.1.0",
    "typescript": "^4.7.4",
    "xo": "^0.39.1",
    "@types/react-dom": "^18.0.5",
    "prettier": "^2.7.1",
    "@commander/ui-kit": "*",
    "@commander/utilities": "*"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/public/build"
        ]
      }
    }
  }
}
